{% highlight bash %}
export IDEA_HOME=/home/q/idea
export M2_HOME=/home/q/maven
export JAVA_BASE=/home/q/java/jdk1.7.0_65
export QSYS_ROOT=/home/q
export WORK_ROOT=/home/work
export WORKSPACE=/home/work/workspace
export JAVA_HOME=/home/q/java/default
export TOMCAT_HOME=/home/q/tomcat
export ANDROID_HOME=/home/q/android/android-sdk-linux
export GRADLE_HOME=/home/q/android/gradle-1.12
export PATH=$PATH:$JAVA_HOME/bin:$M2_HOME/bin:$IDEA_HOME/bin:$GRADLE_HOME/bin:$ANDROID_HOME/platform-tools:$ANDROID_HOME/tools
export CLASSPATH=.:$JAVA_HOME/lib/dt.jar:$JAVA_HOME/lib/tools.jar
